<p id="notice"><%= notice %></p>
<%= form_tag users_path, :method => 'get'  do %>
   <%= text_field_tag :search, params[:search]%>
   <%= submit_tag "Search", :name => nil  %>
 <% end %>

<h1>Users</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <%= @users.inspect %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
